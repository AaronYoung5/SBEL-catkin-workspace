namespace SynInterfaceMessage;

union Type { Camera, Lidar, GPS, IMU, Control, Time, Exit }

table Messages {
  messages:[Message];
  time:Message;
}

table Message {
  message:Type;
  id:string;
}

table Camera {
  height: int;
  width: int;
  bytes_per_pixel:int;
  data:[ubyte];
}

table Lidar {}

table GPS {}

table IMU {}

table Control {
  throttle: float;
  steering: float;
  braking: float;
}

table Time {
  t:float;
}

table Exit {
  code:int;
}

root_type Messages;
